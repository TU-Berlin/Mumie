<sum:summary arrange="free"
             xmlns:sum="http://www.mumie.net/xml-namespace/document/summary"
             xmlns:mtx="http://www.mumie.net/xml-namespace/mmtex">
  <sum:ifstate state="work">
    <mtx:par>
      Zustand des Arbeitsblatts = work
    </mtx:par>
  </sum:ifstate>
  <sum:ifstate state="feedback">
    <mtx:par>
      Zustand des Arbeitsblatts = feedback
    </mtx:par>
    <sum:ifpoints min="0" max="6">
      <mtx:par>
        (A) Punktzahl zwischen 0 und 6
      </mtx:par>
    </sum:ifpoints>
    <sum:ifpoints min="6.1" max="10">
      <mtx:par>
        (A) Punktzahl zwischen 6.1 und 10
      </mtx:par>
    </sum:ifpoints>
    <sum:ifpoints min="10.1" max="max">
      <mtx:par>
        (A) Punktzahl zwischen 10.1 und maximaler Punktzahl
      </mtx:par>
    </sum:ifpoints>
    <sum:ifpoints min="0" max="6">
      <mtx:par>
        (B) Punktzahl zwischen 0 und 6
      </mtx:par>
    </sum:ifpoints>
    <sum:ifpoints min="6.1" max="10">
      <mtx:par>
        (B) Punktzahl zwischen 6.1 und 10
      </mtx:par>
    </sum:ifpoints>
    <sum:ifpoints min="10.1" max="max">
      <mtx:par>
        (B) Punktzahl zwischen 10.1 und maximaler Punktzahl
      </mtx:par>
    </sum:ifpoints>
  </sum:ifstate>
  <sum:iftimeframe relation="inside">
    <mtx:par>
      W&#228;hrend des Bearbeitungszeitraums
    </mtx:par>
  </sum:iftimeframe>
  <sum:iftimeframe relation="after">
    <mtx:par>
      Nach dem Bearbeitungszeitraum
    </mtx:par>
  </sum:iftimeframe>
</sum:summary>
