<xsl:stylesheet version="1.0"
                xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

<!-- Category names: -->
<xsl:param name="i18n-definition-name">Definition</xsl:param>
<xsl:param name="i18n-theorem-name">Theorem</xsl:param>
<xsl:param name="i18n-lemma-name">Lemma</xsl:param>
<xsl:param name="i18n-application-name">Application</xsl:param>
<xsl:param name="i18n-motivation-name">Motivation</xsl:param>
<xsl:param name="i18n-algorithm-name">Algorithm</xsl:param>
<xsl:param name="i18n-proof-name">Proof</xsl:param>
<xsl:param name="i18n-visualization-name">Visualization</xsl:param>
<xsl:param name="i18n-deduction-name">Deduction</xsl:param>
<xsl:param name="i18n-remark-name">Remark</xsl:param>
<xsl:param name="i18n-history-name">History</xsl:param>
<xsl:param name="i18n-example-name">Example</xsl:param>

<!-- Text before lists of propositions: -->
<xsl:param name="i18n-text-proplist-plain">Then follows:</xsl:param>

<!-- Text before lists of equivalent propositions: -->
<xsl:param name="i18n-text-proplist-equivalent">
  <xsl:text>Then the following conclusions are equivalent</xsl:text>
</xsl:param>

<!-- Remarks headline: -->
<xsl:param name="i18n-remarks-headline">Remarks</xsl:param>

<!-- Remark headline: -->
<xsl:param name="i18n-remark-headline">Remark</xsl:param>

<!-- Algorithm input label: -->
<xsl:param name="i18n-algorithm-input">Input:</xsl:param>

<!-- Algorithm output label: -->
<xsl:param name="i18n-algorithm-output">Output:</xsl:param>

<!-- Info page link: (DEPRECATED, now moved to xsl_util) -->
<xsl:param name="i18n-info">Metainfos</xsl:param>
<xsl:param name="i18n-info-expl">Page with metainformations for this document</xsl:param>

<xsl:param name="i18n-close-window">Close window</xsl:param>

</xsl:stylesheet>