<xsl:stylesheet version="1.0"
                xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
  
  <xsl:param name="i18n-capacity">Kapazit&#228;t</xsl:param>
  <xsl:param name="i18n-change">&#196;ndern</xsl:param>
  <xsl:param name="i18n-class">Lehrveranst.</xsl:param>
  <xsl:param name="i18n-classes">Lehrveranstaltung</xsl:param>
  <xsl:param name="i18n-courses">Kurse</xsl:param>
  <xsl:param name="i18n-created-new-tutorial">Tutorium erfolgreich angelegt.</xsl:param>
  <xsl:param name="i18n-description">Beschreibung</xsl:param>
  <xsl:param name="i18n-edit-tutorial">Tutorium bearbeiten</xsl:param>
  <xsl:param name="i18n-error">Fehler</xsl:param>
  <xsl:param name="i18n-invalid-capacity-value">
    <xsl:text>Ung&#252;ltiger Kapazit&#228;ts-Wert. Die Kapazit&#228;t muss eine nicht-negative
      ganze Zahl sein</xsl:text>
  </xsl:param>
  <xsl:param name="i18n-modified-tutorial">Tutoriumdaten erfolgreich ge&#228;ndert</xsl:param>
  <xsl:param name="i18n-name">Name</xsl:param>
  <xsl:param name="i18n-name-missing">Name fehlt</xsl:param>
  <xsl:param name="i18n-negative-capacity">
    <xsl:text>Ung&#252;ltiger Kapazit&#228;ts-Wert. Die Kapazit&#228;t darf nicht negativ sein</xsl:text>
  </xsl:param>
  <xsl:param name="i18n-new-tutorial">Neues Tutorium</xsl:param>
  <xsl:param name="i18n-occupancy">Belegung</xsl:param>
  <xsl:param name="i18n-submit">Abschicken</xsl:param>
  <xsl:param name="i18n-tutor">Tutor</xsl:param>
  <xsl:param name="i18n-tutorials">Tutorien</xsl:param>
  
  <xsl:template name="i18n-hints">
    <xsl:text>Mit </xsl:text>
    <xsl:call-template name="mark-as-mandatory"/>
    <xsl:text> gekennzeichnete Felder sind Pflichtfelder.</xsl:text>
  </xsl:template>
  
</xsl:stylesheet>
